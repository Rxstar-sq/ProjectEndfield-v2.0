 <!-- Bastet_Civet芭丝特·灵猫发型 -->
 <Item identifier="ERWA_Bastet_Civet_HairStyle" category="Equipment" tags="smallitem,clothing" cargocontaineridentifier="crate_endfield" scale="0.5">
  <PreferredContainer primary="crewcab" amount="1" spawnprobability="0.1" notcampaign="true"/>
  <!--价格 (Erthex)-->
  <Price baseprice="150" sold="false"/>
  <!--合成 (Erthex)-->
  <Fabricate suitablefabricators="fabricator_endfield" displayname="购买" requiredtime="5.0" requiredmoney="150" fabricationlimit="1"/>
  <Fabricate suitablefabricators="fabricator_endfield" displayname="合成" requiredtime="5.0">
   <RequiredItem identifier="organicfiber" amount="1"/>
  </Fabricate>
  <!--分解 (Erthex)-->
  <Deconstruct time="2.5"/>
  <!--贴图 (Erthex)-->
  <InventoryIcon texture="%ModDir%/Content_Er/sprite/box_hair.png" sourcerect="0,0,128,128" origin="0.5,0.5"/>
  <Sprite texture="%ModDir%/Content_Er/sprite/box_hair.png" sourcerect="0,0,128,128" origin="0.5,0.5" depth="0.55"/>
  <!--物理 (Erthex)-->
  <Body width="118" height="112" density="15" friction="0.8" restitution="0.0"/>
  <!-- 穿着效果 -->
  <Wearable limbtype="Head" slots="Any,Headset" msg="ItemMsgPickUpSelect">
   <sprite name="Bastet_Civet_Hair_Sprite" texture="%ModDir%/Content/Items/Clothings/AetherGazer/HairStyles/Bastet_Civet_Hair_Sprite.png" limb="Head" hidewearablesoftype="Hair" hidelimb="false" inheritlimbdepth="true" inheritscale="true" ignorelimbscale="true" scale="0.535" hideotherwearables="false" sourcerect="0,0,320,456" origin="0.55625,0.415"/>
  </Wearable>
  <!-- 物品栏(自注射) -->
  <ItemContainer capacity="1" maxstacksize="1" autoinject="true">
   <SlotIcon slotindex="0" texture="Content/UI/StatusMonitorUI.png" sourcerect="384,448,64,64" origin="0.5,0.5"/>
   <Containable items="chem,syringe"/>
  </ItemContainer>
  <!-- 语音通话效果 -->
  <WifiComponent range="35000.0" LinkToChat="true" MinChatMessageInterval="0.0"/>
 </Item>
